[Unit]
Description=Rebecca Node Service
Documentation=https://github.com/rebeccapanel/Rebecca-node
After=network.target nss-lookup.target

[Service]
ExecStart=/usr/bin/env python3 /var/lib/rebecca-node/main.py
Restart=on-failure
WorkingDirectory=/var/lib/rebecca-node

[Install]
WantedBy=multi-user.target
